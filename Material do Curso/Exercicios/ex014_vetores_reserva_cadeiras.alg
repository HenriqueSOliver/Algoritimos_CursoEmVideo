algoritmo "semnome"
// Função :
// Autor :
// Data : 12/12/2020
// Seção de Declarações 
var
   Local: vetor [1..5] de Caracter
   
   c: Inteiro
inicio

EscrevaL("Reserva de cadeiras")
EscrevaL("-------------------")

Para c <- 1 ate 5 faca
  Escreva("Reserve a cadeira: ")
  Leia(Local[c])
FimPara



fimalgoritmo
