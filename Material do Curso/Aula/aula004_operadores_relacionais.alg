algoritmo "semnome"
// Função : Aula04
// Autor :
// Data : 24/06/2020
// Seção de Declarações 
var
a, b, c: inteiro


inicio
// Seção de Comandos
a <- 2
b <- 3
c <- 5

escreval(a>b)
escreval(a=b)
escreval(a<>b)
escreval(a>2)
escreval(a>=2)
escreval(c=a+b)
escreval(c>=b^a)
escreval(c<=b^a)
escreval(b<=c%2)
escreval(1=c%2)


 
fimalgoritmo
