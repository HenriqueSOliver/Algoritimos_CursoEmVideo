algoritmo "semnome"
// Função :
// Autor :
// Data : 10/07/2020
// Seção de Declarações 
var
   v : vetor [1..6] de inteiro
   c: inteiro
inicio
// Seção de Comandos 
   para c <- 1 ate 6 faca
        escreva("Digite o ", c,"º valor: ")
         leia(v[c])
   fimpara
   
   para c <- 1 ate 6 faca
        escreva ("{", v[c] ,"}")
   fimpara
fimalgoritmo
